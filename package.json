    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "redux-logger": "^3.0.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.90.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^7.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^1.0.2"
