{
  "name": "react-antd-typescript-starter",
  "version": "0.1.0",
  "private": true,
  "description": "React antd typescript template",
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx --fix src/**/*.js --fix  src/**/*.jsx",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx ./src",
    "fmt": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,less}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.2.3",
    "axios": "^1.3.4",
    "js-cookie": "^3.0.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "get-sass-vars": "^3.4.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "sass": "^1.58.3",
    "typescript": "*",
    "vite": "^4.1.0"
  }
}
