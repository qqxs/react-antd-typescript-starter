    "@types/node": "^18.14.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-legacy": "^5.2.0",
