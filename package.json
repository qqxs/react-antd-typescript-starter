{
  "name": "react-antd-typescript-starter",
  "version": "0.2.0",
  "private": false,
  "description": "React antd typescript template",
  "author": "shineshao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.3",
    "axios": "^1.2.0",
    "js-cookie": "^2.2.1",
    "particles.js": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "dll": "webpack -p --progress --config webpack.config.dll.js",
    "dev": "npm run start",
    "build": "cross-env GENERATE_SOURCEMAP=false CI=false craco build",
    "test": "craco test",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx --fix src/**/*.js --fix  src/**/*.jsx",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,less}\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-plugin-import": "^1.13.5",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^8.0.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.3",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.56.1",
    "webpack-cli": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "prettier --write"
    ],
    "*.+(json|css|scss|less|md)": [
      "prettier --write"
    ]
  }
}
