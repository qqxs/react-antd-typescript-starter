    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "immer": "^10.0.3",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
