    "antd": "^5.27.3",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.28.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
