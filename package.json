{
  "name": "react-antd-typescript-starter",
  "version": "0.2.0",
  "private": false,
  "description": "React antd typescript template",
  "author": "shineshao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "antd": "^5.0.3",
    "js-cookie": "^2.2.1",
    "particles.js": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "start": "craco start",
    "dll": "webpack -p --progress --config webpack.config.dll.js",
    "dev": "npm run start",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx --fix src/**/*.js --fix  src/**/*.jsx",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,less}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/js-cookie": "^2.2.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "craco-less": "^1.17.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-vars-to-json2": "^0.0.3",
    "shelljs": "^0.8.4",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "prettier --write"
    ],
    "*.+(json|css|scss|less|md)": [
      "prettier --write"
    ]
  }
}
