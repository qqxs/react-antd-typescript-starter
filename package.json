{
  "name": "react-antd-typescript-starter",
  "version": "0.9.4",
  "private": true,
  "description": "React antd5 typescript redux template",
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "analyzer": "tsc && cross-env ANALYZER=true vite build",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx",
    "lint": "eslint --ext .ts,tsx,.js,.jsx ./src",
    "fmt": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,cjs}\"",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:coverage": "jest --silent --watchAll=false --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ezuikit/utils-logger": "1.0.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@sentry/react": "^7.118.0",
    "antd": "^5.19.1",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.1",
    "@sentry/browser": "^8.15.0",
    "@skax/camel": "^0.2.6",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.10.1",
    "get-sass-vars": "^4.0.2",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.5.16",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.6",
    "terser": "^5.31.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vite": "^5.3.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.20.0"
  },
  "engines": {
    "node": ">=18"
  },
  "bugs": {
