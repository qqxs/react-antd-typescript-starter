{
  "name": "react-antd-typescript-starter",
  "version": "0.3.5",
  "private": true,
  "description": "React antd5 typescript redux template",
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "analyzer": "tsc && cross-env ANALYZER=true vite build",
    "fix": "eslint --fix src/**/*.ts --fix  src/**/*.tsx",
    "lint": "eslint --ext .ts --ext .tsx --ext .js --ext .jsx ./src",
    "fmt": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest",
    "test:coverage": "jest --silent --watchAll=false --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^7.42.0",
    "@sentry/tracing": "^7.42.0",
    "antd": "^5.2.3",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "immer": "^9.0.19",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.4.4",
    "@skax/camel": "^0.2.1",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "get-sass-vars": "^3.4.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "only-allow": "^1.1.1",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.3",
    "terser": "^5.16.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-compression": "^0.5.1",
