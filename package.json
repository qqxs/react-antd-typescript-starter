    "terser": "^5.26.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2",
    "vite": "^5.0.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.17.4"
